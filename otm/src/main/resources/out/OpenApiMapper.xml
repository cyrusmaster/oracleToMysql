		<result column="SQDN_NAME" jdbcType="VARCHAR" property="sqdnName"></result> 
		<!-- 是否留学 --> 
		<result column="FGN_FLAG" jdbcType="VARCHAR" property="fgnFlag"></result> 
	</resultMap> 
 
	<!--学生Id与姓名表映射--> 
	<resultMap id="IdListDTO" type="com.hzsun.zbp.hsstuportrait.model.dto.IdListDTO"> 
		<!-- 学号 --> 
		<result column="STU_NO" jdbcType="VARCHAR" property="stuNo"></result> 
		<!-- 姓名 --> 
		<result column="STU_NAME" jdbcType="VARCHAR" property="stuName"></result> 
	</resultMap> 
 
<!--map--> 
 
	<!--获取单个本科生基本信息查询--> 
	<select id="getjbxx" resultMap="JBXXDTO"> 
		SELECT STU_NO,STU_NAME,CARD_TYPE,CARD_NO,DATE_FORMAT( BD_DAY,'%Y-%m-%d' ) BD_DAY,NATIVE_PLACE,STU_NATION,STU_STATE,STU_POL,BEFORE_NAME,STU_SEX,NAME_PHNTC 
		FROM ${tableName} 
		WHERE STU_NO = '${id}' 
	</select> 
 
	<!--获取单个本科生学籍信息查询--> 
	<select id="getxjxx" resultMap="XJXXDTO"> 
		SELECT STU_NO,CLASS_NAME,COLL_NAME,SPEC_NAME,GRADE_NO,UGRD_STATUS STU_STATUS,DVLP_LEVEL,ENTER_MON,STU_TYPE,SCH_FLAG,DORM_FLAG,DATE_FORMAT( SCH_DATE,'%Y-%m-%d' ) SCH_DATE,CMPS_NAME,SQDN_NAME 
		FROM ${tableName} 
		WHERE STU_NO = '${id}' 
	</select> 
 
	<!--获取单个本科生结束学业信息查询--> 
	<select id="getjsxy" resultMap="JSXYDTO"> 
		SELECT STU_ID,FINISH_SCH_WORK_MON,FINISH_SCH_WORK_YEAR_CODE,FINISH_SCH_WORK_YEAR_REASON,GRAD_CREDT_NO,DEGREE_CREDT_NO,FINISH_SCH_WORK_CREDT_NO,DIVIDE_DIRECT_CODE,DATE_FORMAT( REISSUE_GRAD_DATE,'%Y-%m-%d' ) REISSUE_GRAD_DATE,REISSUE_GRAD_REASON,MINOR_DEGREE_CREDT_NO,MINOR_SPEC_CREDT_NO,MINOR_FINISH_SCH_WORK_CREDT_NO 
		FROM ${tableName} 
		WHERE STU_ID = '${id}' 
	</select> 
 
	<!--获取单个本科生学位、学历查询--> 
	<select id="getxwxl" resultMap="XWXLDTO"> 
		SELECT STU_NO,DATE_FORMAT( DEGREE_DAY,'%Y-%m-%d' ) DEGREE_DAY,DEGREE_CREDT_NO,STUDY_MODEL,DATE_FORMAT( PLAN_GRAD_DAY,'%Y-%m-%d' ) PLAN_GRAD_DAY,EDU_SYSTEM 
		FROM ${tableName} 
		WHERE STU_NO = '${id}' 
	</select> 
 
	<!--获取单个本科生家庭通讯方式查询--> 
	<select id="getjttx" resultMap="JTTXDTO"> 
		SELECT STU_ID,FAMILY_ZONE,FAMILY_ADDR,FAMILY_POST_CODE,FAMILY_TEL,FAMILY_EMAIL_ADDR 
		FROM ${tableName} 
		WHERE STU_ID = '${id}' 
	</select> 
 
	<!--获取单个本科生家庭成员查询--> 
	<select id="getjtcy" resultMap="JTCYDTO"> 
		SELECT STU_ID,STU_NO,TEL_NO,EMAIL_ADDR,NATION_NAME,STU_CTRY,HEALTH_STATUS,RELT_NAME,MEM_NAME,BD_MON,MEM_WORK_UNIT,WORK_NAME,SPEC_WORK,WORK_RANK 
		FROM ${tableName} 
		WHERE STU_NO = '${id}' 
	</select> 
 
	<!--获取单个本科生个人通讯信息查询--> 
	<select id="getgrtx" resultMap="GRTXDTO"> 
		SELECT STU_ID,POST_CODE,COMM_ADDR,TEL_NO,MOBILE_NO,FAX_TEL,EMAIL_ADDR,NET_ADDR,IMM_COMM_NO 
		FROM ${tableName} 
		WHERE STU_ID = '${id}' 
	</select> 
 
	<!--获取单个本科生住宿信息查询--> 
	<select id="getxszs" resultMap="XSZSDTO"> 
		SELECT STU_ID,CMPS_NAME,BUILD_NAME,BUILD_ROOM_NO,BED_NO,DORM_TEL,DATE_FORMAT( IN_DAY,'%Y-%m-%d' ) IN_DAY,DATE_FORMAT( OUT_DAY,'%Y-%m-%d' ) OUT_DAY,OUT_SCH_ADDR,HOUSE_TEL,STU_ADDR 
		FROM ${tableName} 
		WHERE STU_NO = '${id}' 
	</select> 
 
	<!--获取单个本科生户口状况查询--> 
	<select id="gethkzk" resultMap="HKZKDTO"> 
		SELECT STU_ID,ENTER_HOUSEHOLD_CLASS,HOUSEHOLD_ORIGIN_PLACE_ADDR,HOUSEHOLD_ORIGIN_PRVN,ENTER_HOUSEHOLD_ORIGIN_CLASS,MOVE_IN_FLAG,DEAL_CODE,DATE_FORMAT( DEAL_DAY,'%Y-%m-%d' ) DEAL_DAY,HOUSEHOLD_IN_SCH_FLAG,DATE_FORMAT( MOVE_OUT_DAY,'%Y-%m-%d' ) MOVE_OUT_DAY 
		FROM ${tableName} 
		WHERE STU_ID = '${id}' 
	</select> 
 
	<!--获取单个本科生来源数据查询--> 
	<select id="getxsly" resultMap="XSLYDTO"> 
		SELECT STU_NO,ENTER_MON,EDU_SYSTEM,STUDY_WAY_CODE,STUDY_WAY,STU_ORIGIN,ENTER_WAY_CODE,ENTER_WAY,SRC_ZONE,SCH_NAME 
		FROM ${tableName} 
		WHERE STU_NO = '${id}' 
	</select> 
 
	<!--获取本科生院系所统计信息查询--> 
	<select id="getyxtj" resultMap="YXTJDTO"> 
		SELECT COLL_ID,UNIT_ID,DPND_UNIT_NO,UNIT_NAME,SHORT_NAME,STU_NUM 
		FROM (SELECT COLL_ID,UNIT_ID,DPND_UNIT_NO,UNIT_NAME,SHORT_NAME FROM ${tableNameApp} WHERE COLL_ID = '${paramId}' LIMIT 1 OFFSET 0) t1, 
			(SELECT SUM(STU_NUM) STU_NUM FROM ${tableNameD} WHERE COLL_NO = '${paramId}' ) t2 
	</select> 
 
	<!--获取本科生专业统计信息查询--> 
	<select id="getzytj" resultMap="ZYTJDTO"> 
		SELECT SPEC_NAME,SUM(STU_NUM) 
		FROM ${tableName} 
		WHERE SPEC_CODE = '${id}' 
	</select> 
 
	<!--获取本科生年级统计信息查询--> 
	<select id="getnjtj" resultMap="NJTJDTO"> 
		SELECT GRADE_NAME,SUM(STU_NUM) 
		FROM ${tableName} 
		WHERE grade_no = '${id}' 
	</select> 
 
	<!--获取本科生民族统计信息查询--> 
	<select id="getmztj" resultMap="MZTJDTO"> 
		SELECT STU_NATION,SUM(STU_NUM) 
		FROM ${tableName} 
		WHERE NATION_CODE = '${id}' 
		AND DVLP_LEVEL_CODE = '100' 
	</select> 
 
	<!--获取班级本科生基本信息查询--> 
	<select id="getbjtj" resultMap="BJTJDTO"> 
		SELECT t1.STU_NO,CARD_TYPE,CARD_NO,DATE_FORMAT( BD_DAY,'%Y-%m-%d' ) BD_DAY,NATIVE_PLACE,STU_NATION,STU_STATE,STU_POL,BEFORE_NAME,STU_SEX,SEX_CODE,NAME_PHNTC 
		FROM ${tableNameInfo} t1,${tableNameSch} t2 
		WHERE CLASS_NO = '${id}' 
		AND t1.STU_NO=t2.STU_NO 
		AND UGRD_PGRD_DVLP_LEVEL_CODE = '1' 
	</select> 
 
	<!--获取中队本科生学籍信息查询--> 
	<select id="getzdtj" resultMap="ZDTJDTO"> 
		SELECT t1.STU_NO STU_NO,t1.CLASS_NAME CLASS_NAME,t1.COLL_NAME COLL_NAME,t1.SPEC_NAME SPEC_NAME, 
		t1.GRADE_NO GRADE_NO,t1.UGRD_STATUS STU_STATUS,t1.DVLP_LEVEL DVLP_LEVEL,t1.ENTER_MON ENTER_MON, 
		t1.STU_TYPE STU_TYPE,t1.SCH_FLAG SCH_FLAG,t1.DORM_FLAG DORM_FLAG,DATE_FORMAT( t1.SCH_DATE,'%Y-%m-%d' ) SCH_DATE, 
		t1.CMPS_NAME CMPS_NAME,t1.SQDN_NAME SQDN_NAME 
		FROM ${tableNameSch} t1, 
			(SELECT STU_NO 
			FROM ${tableNameInst} 
			WHERE SQDN_NO = '${id}') t2 
		WHERE t1.STU_NO=t2.STU_NO 
		AND UGRD_PGRD_DVLP_LEVEL_CODE = '1' 
	</select> 
 
	<!--获取校本科生学籍信息查询--> 
	<select id="getxjtj" resultMap="XJTJDTO"> 
		SELECT STU_NO,CLASS_NAME,COLL_NAME,SPEC_NAME,GRADE_NO,UGRD_STATUS STU_STATUS,DVLP_STATUS,ENTER_MON, 
		STU_TYPE,SCH_FLAG,DORM_FLAG,DATE_FORMAT( SCH_DATE,'%Y-%m-%d' ) SCH_DATE,CMPS_NAME,SQDN_NAME,FGN_FLAG 
		FROM ${tableName} 
		WHERE UGRD_PGRD_DVLP_LEVEL_CODE = '1' 
	</select> 
 
	<!--获取学生Id与姓名表--> 
	<select id="listIds" resultMap="IdListDTO"> 
		select t2.stu_no,t2.stu_name 
		from (select stu_no 
			from ${tableNameSch} 
			where sch_flag='是') t1,${tableNameBase} t2 
		<if test="paramId != null"> 
			where t2.stu_no like '%${paramId}%' 
		</if> 
		<if test="paramName != null"> 
			where t2.stu_name like '%${paramName}%' 
		</if> 
		and t1.stu_no = t2.stu_no 
	</select> 
 
<!--sql--> 
 
	<!--获取表名--> 
	<select id="getTableName" resultType="java.lang.String"> 
        select tablename 
        from pg_tables 
        where schemaname='public' 
        and tablename LIKE '${prefix}%' 
        order by tablename 
        desc limit 1 offset 0 
    </select> 
</mapper> 
